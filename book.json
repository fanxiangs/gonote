    "code": {
      "copyButtons": true
    },
    "disqus": {
      "shortName": "fanxiang"
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": false
    },
    "github": {
      "url": "https://github.com/fanxiangs"
    }
  }
}