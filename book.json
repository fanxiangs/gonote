{
  "root": ".",
  "author": "Xiang Fan",
  "plugins": [
    "anchor-navigation-ex",
    "code",
    "disqus",
    "splitter",
    "toc",
    "simple-page-toc",
    "github",
    "expandable-chapters-small",
    "chapter-fold"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": true,
      "showGoTop": true
